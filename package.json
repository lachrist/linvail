{
  "name": "linvail",
  "description": "Access Control System",
  "version": "6.0.12",
  "author": {
    "name": "Laurent Christophe",
    "email": "laurent.christophe.terken@gmail.com"
  },
  "files": [
    "lib"
  ],
  "main": "lib/_.mjs",
  "types": "lib/_.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lachrist/linvail.git"
  },
  "scripts": {
    "lint": "npx tsc && npx prettier -c '**/*.(mjs|d.ts)' && npx eslint 'lint/**/*.mjs' 'lib/**/*.mjs' 'lib/**/*.d.ts' 'bin/**/*.mjs'",
    "format": "npx prettier -w '**/*.(mjs|d.ts)'",
    "test:all": "sh test/exec.sh",
    "test:one": "node test/deploy.mjs",
    "test:one:cov": "npx c8 -- node test/deploy.mjs",
    "test:one:cov:html": "npx c8 --reporter=html -- node test/deploy.mjs"
  },
  "homepage": "https://github.com/lachrist/linvail",
  "license": "MIT",
  "keywords": [
    "Membrane",
    "Control Access System",
    "Proxy",
    "Aran"
  ],
  "dependencies": {
    "acorn": "^8.14.0",
    "aran": "^5.0.0",
    "astring": "^1.9.0",
    "virtual-proxy": "^0.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/source-map": "^0.5.2",
    "c8": "^10.1.3",
    "eslint": "^9.16.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1"
  }
}
